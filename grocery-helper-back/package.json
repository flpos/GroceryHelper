{
  "name": "grocery-helper-back",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.7.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.4",
    "@types/mongoose": "^5.7.8",
    "node-ts": "^5.0.5",
    "nodemon": "^1.19.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "tsc",
    "prestart": "yarn build",
    "start": "node dist/server.js",
    "dev": "nodemon",
    "debug": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "debug:watch": "nodemon yarn debug"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
